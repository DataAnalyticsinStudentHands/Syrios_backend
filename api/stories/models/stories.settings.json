{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "name": "Stories",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "zone": {
      "type": "dynamiczone",
      "components": [
        "frame.title",
        "frame.endframe",
        "frame.frame1",
        "frame.frame2",
        "frame.frame3",
        "frame.frame4",
        "frame.frame5"
      ]
    },
    "story_name": {
      "type": "string",
      "required": true,
      "maxLength": 64
    },
    "story_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    }
  }
}
